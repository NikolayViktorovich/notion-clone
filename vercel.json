{
  "version": 2,
  "builds": [
    {
      "src": "build/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/cohere.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cohere",
      "dest": "/api/cohere.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}